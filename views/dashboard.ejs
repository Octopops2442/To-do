<%- include("_loginHeader"); -%> <% if(data.length > 0){ for(var i = 0; i <
data.length; i++) { %>
<tr>
  <th scope="row"><%= "Task: " + (i + 1)%></th>
  <br /><br />
  <td><%= data[i].email %></td>
  <br /><br />
  <td><%= data[i].task %></td>
  <br /><br />
  <td><%= data[i].time %></td>
  <br /><br />
  <td><%= data[i].date %></td>
  <br /><br />
</tr>
<% } }else{ %>
<tr>
  <td colspan="3">No Tasks</td>
</tr>
<% } %>

<script>
  document.getElementById("dashboard").classList.remove("active");
  document.getElementById("task").classList.remove("active");

  document.getElementById("dashboard").classList.add("active");
</script>

<%- include("_footer"); -%>
